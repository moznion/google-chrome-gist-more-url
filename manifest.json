{
    "name": "gist-more-url",
    "version": "0.0.4",
    "manifest_version": 2,
    "description": "Show extend url to clone (by 'ssh' and 'git') on Gist.",
    "content_scripts": [
        {
          "matches": ["https://gist.github.com/*"],
          "js": ["js/loader.js"],
          "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
      "js/script.js",
      "vendor/js/underscore-min.js"
    ]
}
